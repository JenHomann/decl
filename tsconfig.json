{
    "files": [
        "src/**/*.ts"
    ],
    "compilerOptions": {
        "target": "es5",        
        "noImplicitAny": true,
        "alwaysStrict": true,
        "noImplicitThis": true,
        "noImplicitUseStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true
    }
}